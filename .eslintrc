{
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "import/extensions": [0],
    "import/no-extraneous-dependencies": "error"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}